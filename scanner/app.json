{
    "expo": {
        "name": "Bus Ticket Scanner",
        "slug": "bus-ticket-scanner",
        "version": "1.3.3",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "plugins": [
            [
                "expo-barcode-scanner",
                {
                    "cameraPermission": "Allow 'Bus Ticket System' to access camera."
                }
            ]
        ],
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "infoPlist": {
                "NSCameraUsageDescription": "Allow 'Bus Ticket System' to access camera.",
                "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": ["android.permission.CAMERA"],
            "package": "com.redstream.busticketscanner"
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "name": "Bus Ticket System"
        },
        "owner": "redstream",
        "extra": {
            "eas": {
                "projectId": "966bc21a-864d-4003-9caf-25e353047d62"
            }
        },
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "url": "https://u.expo.dev/966bc21a-864d-4003-9caf-25e353047d62"
        }
    }
}
